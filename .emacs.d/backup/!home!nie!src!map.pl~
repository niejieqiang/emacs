#!/usr/bin/perl -w
#author:niejieqiang

use strict;
use feature('say');

my @array;
while (<DATA>) {
    @array = split /\sk.*?(?<=\s)/, $_;

    #  print map{"$_"}@array;

#    @array = split /s/, $_;
    foreach ( splice( @array, 1 ) ) {
        print( join( " ", @array, $_ ));
    }

}

